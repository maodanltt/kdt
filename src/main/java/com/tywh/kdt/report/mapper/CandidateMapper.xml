<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tywh.kdt.report.mapper.CandidateMapper">

    <select id="queryCandidateList" resultType="candidate">
        select * from candidate
    </select>

    <insert id="add" parameterType="candidate">
        insert into candidate(id,createdate,username,sitename,candidate,jobtitle,jobtype,mobile,communication,appointdate,
        appointtime,onepattern,onepass,oneremark,secondpattern,secondpass,secondremark,offered,entry,remark)
        values
        (#{id},#{createdate},#{username},#{sitename},#{candidate},#{jobtitle},#{jobtype},#{mobile},#{communication},#{appointdate},
         #{appointtime},#{onepattern},#{onepass},#{oneremark},#{secondpattern},#{secondpass},#{secondremark},#{offered},#{entry},#{remark})
    </insert>

</mapper>


