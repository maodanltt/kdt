<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tywh.kdt.api.mapper.RkMapper">

    <select id="queryThdList" parameterType="string" resultType="dbd">
        select fhdbh,sxh,sum(cs) cs,kwbh,bz from xsmxview where fhdbh = #{dh} and fhzt = '已发'
        group by fhdbh,sxh,kwbh,bz
    </select>

</mapper>