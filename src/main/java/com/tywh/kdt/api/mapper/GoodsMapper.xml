<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tywh.kdt.api.mapper.GoodsMapper">
    <select id="queryGoodsBySxh" parameterType="string" resultType="goods">
        select sxh,shum,mbcs,zbh,shuh,gjdj from fxsgk where sxh = #{sxh}
    </select>
</mapper>